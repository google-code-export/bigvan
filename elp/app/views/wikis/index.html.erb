<h1>Listing wikis</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>News?</th>
    <th>Rdoc?</th>
  </tr>

<% for wiki in @wikis %>
  <tr>
    <td><%=h wiki.title %></td>
    <td><%=h wiki.content %></td>
    <td><%=h wiki.is_news %></td>
    <td><%=h wiki.is_rdoc%></td>
    <td><%= link_to 'Show', wiki %></td>
    <td><%= link_to 'Edit', edit_wiki_path(wiki) %></td>
    <td><%= link_to 'Destroy', wiki, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New wiki', new_wiki_path %>
